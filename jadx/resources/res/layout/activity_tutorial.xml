            <TextView android:textSize="0x7f07024f" android:textColor="@color/white" android:gravity="center" android:id="@+id/tut_text" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginBottom="0x7f07022f" android:text="" android:layout_above="@+id/next" android:layout_centerHorizontal="true" android:textAllCaps="true"/>
        </RelativeLayout>
    </RelativeLayout>
</RelativeLayout>
