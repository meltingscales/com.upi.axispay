<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" android:background="@color/colorPrimary" android:fitsSystemWindows="true" android:layout_width="match_parent" android:layout_height="match_parent">
    <Button android:id="@+id/config" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Config"/>
    <RelativeLayout android:layout_width="match_parent" android:layout_height="match_parent">
        <com.upi.axispay.custom.CustomViewPagerr android:id="@+id/container" android:layout_width="match_parent" android:layout_height="match_parent" app:layout_behavior="@string/appbar_scrolling_view_behavior"/>
        <RelativeLayout android:orientation="horizontal" android:padding="0x7f07022e" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginBottom="0x7f0700f3" android:layout_alignParentBottom="true">
            <TextView android:textSize="0x7f07024d" android:textColor="@color/white" android:layout_gravity="center" android:id="@+id/skip" android:padding="0x7f070228" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/skip" android:layout_alignParentBottom="true" android:layout_marginStart="0x7f0700f5" android:layout_alignParentStart="true"/>
