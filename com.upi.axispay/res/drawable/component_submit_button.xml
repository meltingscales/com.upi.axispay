<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="15.0dip" />
            <stroke android:width="1.0dip" android:color="#ff2e8ae6" />
            <gradient android:startColor="#ff2e8ae6" android:endColor="#ff246bb2" android:angle="-90.0" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle">
            <corners android:radius="15.0dip" />
            <stroke android:width="1.0dip" android:color="#ff246bb2" />
            <solid android:color="#ff2e8ae6" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="15.0dip" />
            <stroke android:width="1.0dip" android:color="#ff246bb2" />
            <gradient android:startColor="#ff2e8ae0" android:endColor="#ff246bb9" android:angle="-90.0" />
        </shape>
    </item>
</selector>